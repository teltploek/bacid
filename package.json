{
  "name": "seatcamp",
  "version": "1.0.0",
  "description": "Web-based video chat with friends!",
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "index.js",
  "scripts": {
    "autotest": "mocha --watch -R spec --compilers js:babel/register --recursive test/",
    "lint": "eslint --ext .js ./ && echo Hooray || echo Please fix",
    "prepublish": "npm run test",
    "start": "node index.js",
    "test": "mocha -R spec --compilers js:babel/register --recursive test/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tec27/seatcamp"
  },
  "keywords": [
    "webrtc",
    "video",
    "chat"
  ],
  "author": "Travis Collins <travis@tec27.com> (http://tec27.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tec27/seatcamp/issues"
  },
  "homepage": "https://github.com/tec27/seatcamp",
  "dependencies": {
    "autoprefixer": "^6.0.1",
    "babel": "^5.8.23",
    "babel-runtime": "^5.8.25",
    "babelify": "^6.0.2",
    "brfs": "^1.4.0",
    "browserify": "^11.2.0",
    "browserify-middleware": "^7.0.0",
    "browserify-shim": "^3.8.0",
    "bundle-collapser": "^1.2.1",
    "canonical-host": "0.0.5",
    "concat-stream": "^1.4.6",
    "cookie-parser": "^1.3.4",
    "cuid": "^1.2.4",
    "data-uri-to-blob": "0.0.4",
    "data-uri-to-buffer": "0.0.4",
    "express": "^4.9.8",
    "filmstrip2gif": "^1.0.1",
    "jade": "^1.7.0",
    "postcss": "^5.0.8",
    "prepare-response": "^1.1.2",
    "rimraf": "^2.2.8",
    "serve-static": "^1.10.0",
    "sha1": "^1.1.0",
    "socket.io": "^1.1.0",
    "socket.io-client": "^1.1.0",
    "tokenthrottle": "^1.1.0",
    "twitter-text": "^1.10.0",
    "waypoints": "^3.1.1",
    "aws-sdk": "~2.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "chai": "^3.3.0",
    "eslint": "^1.5.1",
    "eslint-plugin-babel": "^2.1.1",
    "mocha": "^2.3.3",
    "mocha-eslint": "^1.0.0",
    "proxyquire": "^1.7.2",
    "stream-spigot": "^3.0.5"
  },
  "browser": {
    "waypoints": "./node_modules/waypoints/lib/noframework.waypoints.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "optional": [
            "runtime"
          ]
        }
      ],
      "brfs",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "waypoints": "Waypoint"
  }
}
